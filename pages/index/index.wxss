/**index.wxss**/
.fldz{width: 100%}
.fldz_01{background: #ececec;width: 100%;display: block;height: 250px;text-align: center}
.fldz_01 text{display: block}
.fldz_01 text:nth-of-type(1){font-size: 24px;padding-top:20%}
.gengduo{font-size: 14px;color: #444444;width: 90px;line-height: 35px;border:1px #000 solid;display: block;text-align: center}
.fldz_01 .gengduo{margin: auto;margin-top: 20px;}
.fldz video{width:100%;height: 233px;display: block}
.ddk{width: 100%;display: block;margin-top: 10px}
.ddk_01{width: 100%;height: 250px;background:#231816;text-align: center;color: #fff;display: block;line-height: 250px}
.ddk_02 image{width: 100%;}
.qysh{display: flex;padding: 0 15px;margin-left: -15px;justify-content:space-around;flex-wrap:wrap;}
.qysh .qysh_01{width: calc(50% - 15px);margin-left:15px;margin-top: 15px;}
.qysh .qysh_01 image{width: 100%;}
.qysh .qysh_01 .qyes_text{width: 100%;background: #ececec;padding: 15px;box-sizing:border-box;display: block;}
.qysh .qysh_01 .qyes_text rich-text{font-size: 15px;color: #000;font-family: 'youyuan'}
.qysh .qysh_01 .qyes_text text{font-size: 12px;color: #444;line-height: 18px;display: block;}
.qysh .qysh_01 .qyes_text span{width: 50px;height: 1px;background: #999;display: block;margin:10px 0}
.qysh_02{background: #ececec;width: calc(50% - 15px);margin-left:15px;margin-top: 15px;position: relative;}
.qysh_02 text:nth-of-type(1){font-size: 30px;margin-top: 50%;display: block;text-align: center}
.qysh_02 .gengduo{position: absolute;bottom: 20px;right: 20px}
.qyzyzdz{width: 100%;margin-top: 15px}
.qyzyzdz image{width: 100%;}
.lxzx{width: 100%;display: block;margin-top: 15px}
.lxzx image{width: 100%;display: block}
.lxzx text{display: block;background: #ececec;text-align: center;color: #000;font-size: 16px;line-height: 40px;height: 40px}


.title-group{
  background: #f3f3f3;
  text-align: center;
  padding: 30px 15px;
}
.title-group .title{
  font-size: 20px;
  font-family: "黑体";
  margin-bottom: 10px;
}
.title-group .text{
  font-family: "youyuan";
  font-size: 14px;
  line-height: 42rpx;
  color: #666;
}
.select{
  margin-bottom: 20px;
  width: 100%;
  box-sizing: border-box;
}
.first image{
  width: 100%;
  display: block;
}
.second image{
  width: 100%;
  display: block;
}
.three .tu{
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
  padding: 30px 0;
}
.three .li-group{
  text-align: center;
  background: rgba(0,0,0,0.5);
  color: #fff;
  font-family: "youyuan";
  padding: 15px;
  margin: 10px 0;
  box-sizing: border-box;
  width: 100%;
}
.three .li-group .text{
  font-size: 14px;
  line-height: 42rpx;
}
.three .li-group image{
  width: 53px;
  height: 42px;
  display: inline-block;
}
.fourth .title1{
  font-size: 14px;
  font-family: "youyuan";
  background: #000;
  line-height: 32px;
  padding: 0 10px;
  display: inline-block;
  margin-bottom: 5px;
}
.last {
  width: 100%;
  box-sizing: border-box;
}
.last .dl{
  padding: 0 10px;
  width: 100%;
  box-sizing: border-box;
}
.last .dd{
  width: calc(50% - 10px);
  float: left;
  margin: 0 5px;
  margin-bottom: 15px;
  display: block;
  box-sizing: border-box;
}
.last .dd image{
  width: 100%;
  display: block;
}
.last .dd .h4{
  line-height: 20px;
  padding: 10px;
  text-align: center;
  color: #333;
  background: #e9e9e9;
  font-size: 28rpx;
}
.btn7{
  line-height: 36px;
  padding: 5px 0;
  background: #e9e9e9;
  text-align: center;
  font-size: 15px;
  color: #333;
}